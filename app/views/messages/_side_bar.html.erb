<div class="username">

  <div class="chat_create">
    <div class="name">
      <%= link_to current_user.name, edit_user_path(current_user)%>
    </div>

    <div class="create_room">
      <%= link_to "チャットを作成する", new_room_path %>
    </div>
  </div>

</div>


<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="chat_names">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>
</div>